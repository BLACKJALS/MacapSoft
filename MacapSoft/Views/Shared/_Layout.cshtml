<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET Application</title>
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")

</head>
<body>
    <div class="navbar navbar-default navbar-fixed-top navbar-custom" role="navigation" style="background-color:#06052F">
        <div class="container">
            <div class="navbar-header">
                <a class="navbar-brand navbar-brand-left">
                    <img src="~/Content/Imagenes/Logo_CAP_UAN.png" />
                    @*<img src="~/Content/Imagenes/Logo_CAP_UAN.png" />*@
                </a>
            </div>
        </div>  
    </div>
    <br />
    <br />
    <br />
    <br />
    @*<div class="navbar-collapse collapse">
        <ul class="nav navbar-nav">
            <li>CAP</li>
        </ul>
        @Html.Partial("_SchemaPartial")
    </div>*@
    <section>
        <div class="container body-content">
            @RenderBody()
            <hr />
            <footer>
                <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
            </footer>
        </div>
    </section>
    

    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>
